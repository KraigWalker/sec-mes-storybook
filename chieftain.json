    "external_yb_url": "{{ secure_messages_webapp_metadata.urls.external_yb_url }}",
    "external_vm_url": "{{ secure_messages_webapp_metadata.urls.external_vm_url }}",
    "internal_url": "{{ secure_messages_webapp_metadata.urls.internal_url }}",
    "image_name": "secure-messages-webapp",
    "java_heap_size": "{{ base_microservice_bom.java_heap_size }}",
    "name": "secure-messages-webapp",
