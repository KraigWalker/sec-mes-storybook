  "scripts": {
    "nocss": "cross-env brand= webpack --config ./webpack.config.dev.js",
    "build": "npm run prod",
    "prod": "webpack --config ./webpack.config.prod.js",
    "start": "cross-env npm run dev && webpack-dev-server --config ./webpack.config.dev.js",
    "cypress:open": "cypress open",
    "launchBrowserWithConfig": "node ./_tasks/openMEOBrowser",
    "start:advisor": "npm run launchBrowserWithConfig && node ./_tasks/getStaffToken && webpack-dev-server --config ./dev/advisor/dev.config.js",
    "start:ios": "webpack-dev-server --config ./webpack.config.dev.js --env.APP_CONTEXT=ios",
    "start:android": "webpack-dev-server --config ./webpack.config.dev.js --env.APP_CONTEXT=android",
    "dev": "webpack --config ./webpack.config.dev.js",
    "test": "jest",
    "test:ci": "jest",
    "deploy": "deployist",
    "build:lib": "rollup -c",
    "test:watch": "npm test -- --watch",
    "lint": "eslint './src'",
    "lint:fix": "eslint './src' --fix",
    "format": "prettier 'src/**/*.js' 'src/**/*.jsx' 'scripts/**.*'",
    "format:fix": "prettier --write 'src/**/*.js' 'src/**/*.jsx' 'scripts/**.*'",
    "test:coverage": "jest --coverage --u",
    "pack:dev": "rollup -c && npm pack && mv *.tgz node_modules_offline/"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:fix",
      "npm run format:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged && npm run test:ci"
    }
  },
  "auto-changelog": {
    "replaceText": {
      "[\\(]([a-zA-z]+-+\\d+)[\\)]": "([`$1`](https://abouthere.atlassian.net/browse/$1))"
    }
