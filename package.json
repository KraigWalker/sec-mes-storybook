  "scripts": {
    "nocss": "cross-env brand= webpack --config ./webpack.config.dev.js",
    "build": "npm run prod",
    "prod": "webpack --config ./webpack.config.prod.js",
    "start": "cross-env npm run dev && webpack-dev-server --config ./webpack.config.dev.js",
    "cypress:open": "cypress open",
    "launchBrowserWithConfig": "node ./_tasks/openMEOBrowser",
    "start:advisor:disabled_temporarily": "npm run launchBrowserWithConfig && node ./_tasks/getStaffToken && webpack-dev-server --config ./dev/advisor/dev.config.js",
    "start:ios": "webpack-dev-server --config ./webpack.config.dev.js --env.APP_CONTEXT=ios",
    "start:android": "webpack-dev-server --config ./webpack.config.dev.js --env.APP_CONTEXT=android",
    "dev": "webpack --config ./webpack.config.dev.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:ci": "node --experimental-vm-modules node_modules/.bin/jest --watchAll=false --ci --reporters=default --runInBand --no-cache",
    "deploy": "deployist",
    "build:lib": "rollup -c",
    "test:watch": "npm test -- --watch",
    "lint": "eslint './src'",
    "lint:fix": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.jseslint './src' --fix",
    "format": "prettier 'src/**/*.js' ",
    "format:fix": "prettier --write 'src/**/*.js'",
    "test:coverage": "jest --coverage --u",
    "pack:dev": "rollup -c && npm pack && mv *.tgz node_modules_offline/",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "version": "git fetch --tags && npm run changelog"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(lodash-es)/)"
    ],
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run format:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "auto-changelog": {
    "replaceText": {
      "[\\(]([a-zA-z]+-+\\d+)[\\)]": "([`$1`](https://abouthere.atlassian.net/browse/$1))"
    }
  }
